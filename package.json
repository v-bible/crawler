{
  "name": "@v-bible/crawler",
  "version": "1.0.0",
  "description": "A collection of web crawlers to crawl Catholic resources in Vietnamese language",
  "private": true,
  "type": "module",
  "bin": {
    "crawler": "./dist/bin/cli.js"
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsc",
    "dev": "tsx src/bin/cli.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "validate": "pnpm run typecheck && pnpm run format:check && pnpm run lint && pnpm run test:run",
    "completion": "tsx src/bin/bash-complete.ts"
  },
  "dependencies": {
    "@ghostery/adblocker-playwright": "2.13.4",
    "@stdlib/nlp-sentencize": "0.2.4",
    "@stricli/auto-complete": "1.2.5",
    "@stricli/core": "1.2.5",
    "async-retry": "1.3.3",
    "bluebird": "3.7.2",
    "date-fns": "4.1.0",
    "dotenv": "17.2.4",
    "es-toolkit": "1.44.0",
    "fast-csv": "5.0.5",
    "natural": "8.1.0",
    "pdfjs-dist": "5.4.624",
    "playwright": "1.58.2",
    "proper-lockfile": "4.1.2",
    "rehype-parse": "9.0.1",
    "rehype-remark": "10.0.1",
    "rehype-remove-comments": "6.1.1",
    "remark": "15.0.1",
    "remark-gfm": "4.0.1",
    "remark-smartypants": "3.0.2",
    "remark-stringify": "11.0.0",
    "strip-markdown": "6.0.0",
    "undici": "7.21.0",
    "unified": "11.0.5",
    "unist-builder": "4.0.0",
    "wink-eng-lite-web-model": "1.8.1",
    "wink-nlp": "2.4.0",
    "winston": "3.19.0",
    "xast-util-to-xml": "4.0.0",
    "xastscript": "4.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@commitlint/cli": "20.4.1",
    "@commitlint/config-conventional": "20.4.1",
    "@playwright/browser-chromium": "1.58.2",
    "@types/async-retry": "1.4.9",
    "@types/bluebird": "3.5.42",
    "@types/node": "25.2.3",
    "@types/proper-lockfile": "4.1.4",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-playwright": "1.8.3",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  }
}
